  <node
  	pkg="rviz"
	type="rviz"
	name="RVIZ"
	respawn="true">
  </node>   

  <node
    pkg="line_extract"
    type="line_extract_node"
    name="LRF"
	respawn="true">
  </node>

  <node
    pkg="ekf_localize"
    type="ekf_localize_node"
    name="FILTER"
	respawn="true"
    args="-f $(find ekf_localize)/config1.xml -m $(find ekf_localize)/map2.xml"
	output="screen">
  </node>

  <node
    pkg="planner"
    type="planner_circle_run"
    name="CIRCLE"
	output = "screen">
  </node>

