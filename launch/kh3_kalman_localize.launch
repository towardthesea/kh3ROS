<launch>
  <param name="use_sim_time" value="false"/>

  <!-- Generate a fixed frame world at (0.0,0.0,0.0) and odom frame is translated (0.1,0.4,0.0) and rotated -90degree around z axis-->
  <node 
	pkg="tf" 
	type="static_transform_publisher" 
	name="WORLD" 
	args="0.48 0.805 0 0 0 -3.14 world odom 100">
  </node>

  <node
    pkg="rosbag"
    type="record"
    name="RECORD"
	args="-a -o /home/towardthesea/catkin_ws/src/bag/test">
  </node>

  <node
    pkg="playerros_teleop"
    type="playerros_teleop_node"
    name="Keyboard"
	output="screen">
  </node>

  <node 
    pkg="playerros"
    type="playerros_node"
    name="Player_ROS"
	args="-h 192.168.1.150">
  </node> 
